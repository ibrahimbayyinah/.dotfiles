# Source: https://github.com/weasnerb/linux-dots/blob/main/src/.config/wal/templates/hyprpaper.config

# Currently no source or variable support in hyprpaper config, so making the config via wal templating

# Preload 1-n Wallpapers
preload = {wallpaper}

# Set wallpaper(s)
# Currently setting wallpaper on all screens
wallpaper = , {wallpaper}

# disable splash
splash = false

# fully disable ipc
ipc = off
